  "scripts": {
    "precommit": "lint-staged",
    "start": "node scripts/start.js",
    "debug": "node --inspect-brk=9229 scripts/start.js",
    "build": "node scripts/build.js",
    "build-analyse": "cross-env ANALYSIS=true node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "local-server": "http-server ./WebClient -g",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "eslint": "eslint src/**/*.{js,jsx,vue,ts,tsx}",
    "tslint": "tslint src/**/*.{ts,tsx}",
    "prettier": "prettier --config .prettierrc src/**/*.{js,jsx,json,less,ts,tsx}",
    "stylelint": "stylelint 'src/**/*.{css,less,scss}'",
    "ts-jest": "ts-jest config:init",
    "analyse": "cross-env NODE_ENV=production webpack-cli --config ./config/webpack.config.prod.js --profile --json > stats.json",
    "show-analyzer-result": "webpack-bundle-analyzer stats.json"
  },
  "lint-staged": {
    "src/**/*.{js,vue,jsx,ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint",
      "git add"
    ],
    "src/**/*.{css,scss,less}": [
      "prettier --config .prettierrc --write",
      "stylelint --syntax=scss",
      "git add"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "not ie 8-11",
      "chrome >48"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs,ts,tsx}",
      "**/?(*.)(spec|test).{js,jsx,mjs,ts,tsx}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(graphql)$": "<rootDir>/config/jest/graphqlTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "ifvendors(.*)$": "<rootDir>/src/vendors/$1",
      "ifutils(.*)$": "<rootDir>/src/vendors/utils/$1",
      "stsrc(.*)$": "<rootDir>/src/$1",
      "stcontainers(.*)$": "<rootDir>/src/containers/$1",
      "stcomponents(.*)$": "<rootDir>/src/components/$1",
      "stpages(.*)$": "<rootDir>/src/pages/$1",
      "stutils(.*)$": "<rootDir>/src/utils/$1",
      "stassets(.*)$": "<rootDir>/src/assets/$1",
      "stconfig(.*)$": "<rootDir>/src/utils/config/$1",
      "sttypedefine(.*)$": "<rootDir>/src/type-define/$1"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    }
  },
